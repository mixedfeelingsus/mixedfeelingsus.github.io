form {
	position: relative;
}

#backgroundContainer {
   width:100%;
   height:100%;
   background:url("../img/venn.png") center center no-repeat;
}

#intro {
	padding-top: 60px;
	padding-bottom: 60px;
}

#more {
	display: none;
}

#content {
	background-color: rgba(255,255,255,0.6);
	padding: 14px 20px;
	margin-bottom: 40px;
}

#quiz {
	background-color: rgba(255,255,255,0.6);
	padding: 14px 20px;
	margin-bottom: 40px;
	height: 300px;
	text-align:center;
}

#intro h1 {
	font-size: 6.2em;
	text-align: center;
}

#intro p {
	font-size: 2.1em;
}

#button {
	position: absolute;
	top:260px;
	left: 420px;
}

.slider{
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 5px;
	width: 620px;
}

ol.label{
	text-align: left;
	width: 620px;
	margin-top: 10px;
	margin-bottom: 30px;
	margin-left: auto;
	margin-right: auto;
}

ol.label li {
	display: inline;
	float: left;
	font-size: 1.2em;
	color: #666666;
}

.q {
	position: absolute;
	width: 920px;

}

.center {
	text-align: center;
}

.right {
	text-align: right;
}