body {
	font-family: "FrutigerLTStd45Light";
	background: #F4F4F4 url("../img/texture.gif");
	width: 100%;
	height: 100%;
}

#vizContainer{
	margin-top: 40px;
	padding-top: 20px;
	background-color: #ffffff;
	box-shadow: 0px 0px 10px #ccc;
}

h1, h2, h3, h4, h5, h6{
	font-weight: normal;
	margin: 0;
}

h1 {
	font-size: 4.2em;
}

h2 {
	font-size: 3.6em;
}
h3 {
	font-size: 2.4em;
}
h4 {
	font-size: 2.1em;
}
h5 {
	font-size: 1.8em;
}
h6 {
	font-size: 1.6em;
}

p, text {
	font-size: 1.4em;
}

#backgroundContainer {
   width:100%;
   height:100%;
   background:url("../img/venn.png") center center no-repeat;
}

#intro {
	padding-top: 60px;
}

#content {
	background-color: rgba(255,255,255,0.6);
	padding: 14px 20px;
	margin-bottom: 40px;
}

#intro h1 {
	font-size: 6.2em;
	text-align: center;
}


.btn {
  display: inline-block;
  padding: 4px 0px;
  margin-bottom: 0;
  font-size: 1.4em;
  color: #333333;
  text-align: center;
  text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
  vertical-align: middle;
  background-color: #f5f5f5;
  background-image: linear-gradient(top, #f1f1f1, #e6e6e6);
  background-repeat: repeat-x;
  border-color: #e6e6e6 #e6e6e6 #bfbfbf;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  border: 1px solid #cccccc;
  border-bottom-color: #b3b3b3;
  border-radius: 4px;
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 3px rgba(153, 153, 153, 0.65);
  cursor: pointer;
}

.btn:hover,
.btn:active{
  background-color: #e6e6e6;
}

.btn:active {
  background-color: #cccccc;
}

.btn:hover {
  color: #333333;
  text-decoration: none;
  background-color: #e6e6e6;
  background-position: 0 -15px;
  transition: background-position 0.1s linear;
}

.btn:focus {
  outline: thin dotted #333;
  outline: 5px auto -webkit-focus-ring-color;
  outline-offset: -2px;
}

.btn:active {
  background-image: none;
  box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);
  background-color: #e6e6e6;
  outline: 0;
}

.btn:visited {
	color: #333333;
}

.big {
	font-size: 2.8em;
	border-radius: 8px;
}

