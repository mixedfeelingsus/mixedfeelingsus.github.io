#backgroundContainer {
   width:100%;
   height:100%;
   background:url("../img/venn.png") center center no-repeat;
}

#intro {
	padding-top: 60px;
	padding-bottom: 60px;
}

#more {
	display: none;
}

#content {
	background-color: rgba(255,255,255,0.6);
	padding: 14px 20px;
	margin-bottom: 40px;
}

#intro h1 {
	font-size: 6.2em;
	text-align: center;
}

#intro p {
	font-size: 2.1em;
}

.slider{
	margin-bottom: 5px;
}

ol.label{
	margin-top: 10px;
	margin-bottom: 30px;
}

ol.label li {
	display: inline;
	float: left;
	font-size: 1em;
}

.center {
	text-align: center;
}

.right {
	text-align: right;
}
